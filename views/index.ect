		Client.article_view.comment_list.addCommentInput();
	});

	// issue
	$('#sidebar a[href=#issue_week]').click(function() {
		$('#sidebar .issue-tab li').removeClass('select');
		$(this).parent().addClass('select');
		$('#issue_month').hide();
		$('#issue_week').show();
		return false;
	});
	$('#sidebar a[href=#issue_month]').click(function() {
		$('#sidebar .issue-tab li').removeClass('select');
		$(this).parent().addClass('select');
		$('#issue_week').hide();
		$('#issue_month').show();
		return false;
	});

	$('#article-container').show();

	$('#searchbar .search-form a').click(function() {
		var search = $('#searchbar .search-form input.search').val();
		if(!search) {
			alert('검색어를 입력해주세요.');
			$('#searchbar .search-form input.search').focus();
			return;
		}
		var win = window.open('http://heyo.me/writings#list/1/'+encodeURI(search), 'popup-search');
		return false;
	});
	$('#searchbar .search-form input.search').keydown(function(e) {
		if (e.keyCode==13) {
			$('#searchbar .search-form a').click();
			return false;
		}
	});

	//time-trip
	$('#timetrip-date').datepicker({
		dateFormat: 'yy-mm-dd',
		onSelect: function(dateText, inst) {
			location.href = '/d/'+dateText;
