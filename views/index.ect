<script type="text/javascript">
$(function() {
	Client.init();

	Client.article_view.afterRender = function(article) {
		$('#content-view img').each(function() {
			$(this)[0].removeAttribute('width');
			$(this)[0].removeAttribute('height');


			var self = this;
			var src = $(this).attr('src');
			if (!src) src = '';
			if (!$(this)) return;
			if (src.indexOf('dcimg2.dcinside.co.kr')!=-1) {
				var url = $(self).attr('src');
				//var src = 'http://dcimg2.dcinside.com.heyo.me/viewimage.php?url='+encodeURIComponent(url);
				var src = url.replace(/dcimg2\.dcinside\.co\.kr/g, 'image.dcinside.com');
				$(this).hide().after(
					$('<img/>').attr('src', src)
				);				
			} else if (src.indexOf('dcimg1.dcinside.co.kr')!=-1) {
				var url = $(self).attr('src');
				var src = url.replace(/dcimg1\.dcinside\.co\.kr/g, 'image.dcinside.com');
				$(this).hide().after(
					$('<img/>').attr('src', src)
				);				
			} else if (src.indexOf('heyo.me')==-1) {
				$(this).one('error', function() {
					$(this).after($('<a href="'+$(this).attr('src')+'" style="color:#c8c8c8;font:8pt tahoma;" target="_blank">'+$(this).attr('src')+'</a>'));
					$(this).after($('<span style="color:#c8c8c8;"><img src="/img/image-proxy2.gif"/></span>').click(function() {
						var url = $(self).attr('src');
						var src = 'http://heyo.me/image_proxy.php?url='+encodeURIComponent(url);
						$(this).html('').append(
							$('<img/>').attr('src', src)
						);
					}));
					$(this).after($('<span style="color:#c8c8c8;"><img src="/img/image-proxy1.gif"/></span>').click(function() {
						var url = $(self).attr('src');
						var src = 'http://guoo.net/image_proxy.php?url='+encodeURIComponent(url);
						$(this).html('').append(
							$('<img/>').attr('src', src)
						);
					}));
				});
			}
		});
	}
